  <extension point="xbmc.addon.metadata">
    <summary lang="pt">Agenda de Eventos Desportivos</summary>
    <description lang="pt">Listas com canais onde passam eventos desportivos das equipas portuguesas</description>
    <platform>all</platform>
  </extension>
